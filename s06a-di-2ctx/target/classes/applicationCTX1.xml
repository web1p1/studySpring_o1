<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="student1" class="com.wind.s06.Student">
		<constructor-arg value="그레고리펙"></constructor-arg>
		<constructor-arg value="34"></constructor-arg>
		<constructor-arg>
			<list>
				<value>수영</value>
				<value>요리</value>
			</list>
		</constructor-arg>
		
		<property name="height" value="187"></property>
		<property name="weight" value="75"></property>
	</bean>

	<bean id="studentInfo1" class="com.wind.s06.StudentInfo">
		<property name="student" ref="student1"></property>
	</bean>
	
</beans>
