<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="student2" class="com.wind.s06.Student">
		<constructor-arg value="루팡"></constructor-arg>
		<constructor-arg value="28"></constructor-arg>
		<constructor-arg>
			<list>
				<value>줄넘기</value>
				<value>마술</value>
			</list>
		</constructor-arg>
		
		<property name="height" value="175"></property>
		<property name="weight" value="68"></property>
	</bean>

</beans>
